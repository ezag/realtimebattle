# Targets to generate the files if the sgml source exists, otherwise nothing happens 

@SGML2HTML_TRUE@%.html: %.sgml
@SGML2HTML_TRUE@	@if [ -f $< ]; then \
@SGML2HTML_TRUE@		echo "sgml2html --split=0 -c latin $<"; \
@SGML2HTML_TRUE@		sgml2html --split=0 -c latin $<; \
@SGML2HTML_TRUE@	fi
@SGML2HTML_FALSE@%.html: %.sgml
@SGML2HTML_FALSE@	@echo "Warning: $< cannot be generated, sgml2html is missing."; \
@SGML2HTML_FALSE@	touch $@

#	elif [ ! -f $@ ]; then \
#		echo "Error: $@ is missing and cannot be generated"; \
#	fi

