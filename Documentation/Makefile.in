# Targets to generate the files if the sgml source exists, otherwise nothing happens 

@SGML2HTML_TRUE@%.html: %.sgml
@SGML2HTML_TRUE@	@if [ -f $< ]; then \
@SGML2HTML_TRUE@		echo "sgml2html --split=0 -c latin $<"; \
@SGML2HTML_TRUE@		sgml2html --split=0 -c latin $<; \
@SGML2HTML_TRUE@	fi
@SGML2HTML_FALSE@%.html: %.sgml
@SGML2HTML_FALSE@	@echo "Warning: $< cannot be generated, sgml2html is missing."; \
@SGML2HTML_FALSE@	touch $@

#	elif [ ! -f $@ ]; then \
#		echo "Error: $@ is missing and cannot be generated"; \
#	fi

@SGML2HTML_TRUE@RealTimeBattle%.html: RealTimeBattle.sgml
@SGML2HTML_TRUE@	@if [ -f $< ]; then \
@SGML2HTML_TRUE@		echo "sgml2html --imagebuttons -D table=real -c latin $<"; \
@SGML2HTML_TRUE@		sgml2html --imagebuttons -D table=real -c latin $<; \
@SGML2HTML_TRUE@	elif [ ! -f $@ ]; then \
@SGML2HTML_TRUE@		echo "Error: $@ is missing and cannot be generated"; \
@SGML2HTML_TRUE@	fi
@SGML2HTML_FALSE@RealTimeBattle%.html: RealTimeBattle.sgml
@SGML2HTML_FALSE@	@echo "Warning: $@ cannot be generated, sgml2html is missing."; \
@SGML2HTML_FALSE@	touch $@

@SGML2LATEX_TRUE@%.dvi: %.sgml
@SGML2LATEX_TRUE@	@if [ -f $< ]; then \
@SGML2LATEX_TRUE@		echo "sgml2latex  -p $(PAPER_SIZE) -D table=real -c latin -o dvi $<"; \
@SGML2LATEX_TRUE@		sgml2latex  -p $(PAPER_SIZE) -D table=real -c latin  -o dvi $<; \
@SGML2LATEX_TRUE@	elif [ ! -f $@ ]; then \
@SGML2LATEX_TRUE@		echo "Error: $@ is missing and cannot be generated"; \
@SGML2LATEX_TRUE@	fi
@SGML2LATEX_FALSE@%dvi: %.sgml
@SGML2LATEX_FALSE@	@echo "Warning: $@ cannot be generated, sgml2latex is missing."; \
@SGML2LATEX_FALSE@	touch $@

